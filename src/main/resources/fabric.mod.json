{
  "schemaVersion": 1,
  "id": "spwp",
  "version": "${version}",
  "name": "SPWorlds Pay",
  "description": "Mod that allows you to manage your SP/SPm cards directly in Minecraft",
  "authors": [
    "MeiNanziiii"
  ],
  "contact": {
    "homepage": "https://github.com/Miracle-Studio/SPWorlds-Pay",
    "sources": "https://github.com/Miracle-Studio/SPWorlds-Pay",
    "issues": "https://github.com/Miracle-Studio/SPWorlds-Pay/issues"
  },
  "license": "MIT",
  "icon": "assets/spwp/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "ua.mei.spwp.client.SPWorldsPayClient"
    ]
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/Jc4pqjQdBf",
        "modmenu.github_releases": "https://github.com/Miracle-Studio/SPWorlds-Pay/releases",
        "modmenu.modrinth": "https://modrinth.com/mod/spwp"
      }
    }
